public void refreshTable(){
	data.clear();

	String query = "SELECT * from mydb.revision_log";
	pst = conn.prepareStatement(query);
	rs = pst.executeQuery();

	while (rs.next()){
		data.add(new User(
		rs.getString();

		));
		table.setItems(data);

	}
	pst.close();
	rs.close();

}catch(Exception e2){
	System.err.println(e2);
}
